        <param name="ios-package" value="GooglePlus" />
        <param name="onload" value="true" />
    </feature>
    <access origin="https://accounts.google.com/*" />
    <name>IC</name>
    <description>
        Implant Compare official app.
    </description>
    <author email="info@implantcompare.com" href="https://implantcompare.com">
