<?xml version='1.0' encoding='utf-8'?>
<widget id="com.implantcompare.implantcompare" version="2.6.7" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>IC</name>
    <description>
        Implant Compare official app.
    </description>
    <author email="info@implantcompare.com" href="https://implantcompare.com">
        Implant Compare Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <allow-navigation href="*" />
        <icon height="180" src="res/icons/Icon-60@3x.png" width="180" />
        <icon height="60" src="res/icons/Icon-60.png" width="60" />
        <icon height="120" src="res/icons/Icon-60@2x.png" width="120" />
        <icon height="76" src="res/icons/Icon-76.png" width="76" />
        <icon height="152" src="res/icons/Icon-76@2x.png" width="152" />
        <icon height="40" src="res/icons/Icon-40.png" width="40" />
        <icon height="80" src="res/icons/Icon-40@2x.png" width="80" />
        <icon height="57" src="res/icons/Icon.png" width="57" />
        <icon height="114" src="res/icons/Icon@2x.png" width="114" />
        <icon height="72" src="res/icons/Icon-72.png" width="72" />
        <icon height="144" src="res/icons/Icon-72@2x.png" width="144" />
        <icon height="167" src="res/icons/Icon-167.png" width="167" />
        <icon height="29" src="res/icons/Icon-small.png" width="29" />
        <icon height="58" src="res/icons/Icon-small@2x.png" width="58" />
        <icon height="50" src="res/icons/Icon-50.png" width="50" />
        <icon height="100" src="res/icons/Icon-50@2x.png" width="100" />
        <icon height="167" src="res/icons/Icon-83.5@2x.png" width="167" />
        <splash height="480" src="res/splash/Default~iphone.png" width="320" />
        <splash height="1136" src="res/splash/Default-568h@2x~iphone_640x1136.png" width="640" />
        <splash height="960" src="res/splash/Default@2x~iphone_640x960.png" width="640" />
        <splash height="748" src="res/splash/Default-Landscape~ipad_1024x748.png" width="1024" />
        <splash height="768" src="res/splash/Default-Landscape~ipad_1024x768.png" width="1024" />
        <splash height="1536" src="res/splash/Default-Landscape@2x~ipad_2048x1536.png" width="2048" />
        <splash height="2048" src="res/splash/Default-Portrait@2x~ipad_1536x2048.png" width="1536" />
        <splash height="1024" src="res/splash/Default-Portrait~ipad_768x1024.png" width="768" />
        <splash height="750" src="res/splash/Default-750@2x~iphone6-landscape_1334x750.png" width="1334" />
        <splash height="1334" src="res/splash/Default-750@2x~iphone6-portrait_750x1334.png" width="750" />
        <splash height="2208" src="res/splash/Default-1242@3x~iphone6s-portrait_1242x2208.png" width="1242" />
        <splash height="1242" src="res/splash/Default-1242@3x~iphone6s-landscape_2208x1242.png" width="2208" />
    </platform>
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#04334d" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="Fullscreen" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="SplashScreenDelay" value="2000" />
    <preference name="BackgroundColor" value="#04334d" />
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-plugin-browsertab" spec="^0.2.0" />
    <plugin name="cordova-plugin-buildinfo" spec="^1.1.0" />
    <plugin name="cordova-plugin-customurlscheme" spec="^4.2.0">
        <variable name="URL_SCHEME" value="com.implantcompare.implantcompare" />
    </plugin>
    <plugin name="cordova-plugin-facebook4" spec="^1.9.0">
        <variable name="APP_ID" value="1834783516803213" />
        <variable name="APP_NAME" value="IC" />
    </plugin>
    <plugin name="cordova-plugin-firebase" spec="^0.1.21" />
    <plugin name="cordova-plugin-googleplus" spec="^5.1.1">
        <variable name="REVERSED_CLIENT_ID" value="com.googleusercontent.apps.807764475515-lei0cla1lpmpmu6n7s43ad7m7pjm36ot" />
    </plugin>
    <plugin name="cordova-plugin-inappbrowser" spec="^1.7.1" />
    <plugin name="cordova-plugin-splashscreen" spec="^4.0.3" />
    <plugin name="cordova-plugin-statusbar" spec="^2.2.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <plugin name="cordova-universal-links-plugin" spec="^1.2.1" />
</widget>
