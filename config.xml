<?xml version='1.0' encoding='utf-8'?>
<widget id="com.implantcompare.implantcompare" version="2.6.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>IC</name>
    <description>
        Implant Compare official app.
    </description>
    <author email="info@implantcompare.com" href="https://implantcompare.com">
        Implant Compare Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="180" src="res/icons/Icon-60@3x.png" width="180" />
        <icon height="60" src="res/icons/Icon-60.png" width="60" />
        <icon height="120" src="res/icons/Icon-60@2x.png" width="120" />
        <icon height="76" src="res/icons/Icon-76.png" width="76" />
        <icon height="152" src="res/icons/Icon-76@2x.png" width="152" />
        <icon height="40" src="res/icons/Icon-40.png" width="40" />
        <icon height="80" src="res/icons/Icon-40@2x.png" width="80" />
        <icon height="57" src="res/icons/Icon.png" width="57" />
        <icon height="114" src="res/icons/Icon@2x.png" width="114" />
        <icon height="72" src="res/icons/Icon-72.png" width="72" />
        <icon height="144" src="res/icons/Icon-72@2x.png" width="144" />
        <icon height="167" src="res/icons/Icon-167.png" width="167" />
        <icon height="29" src="res/icons/Icon-small.png" width="29" />
        <icon height="58" src="res/icons/Icon-small@2x.png" width="58" />
        <icon height="50" src="res/icons/Icon-50.png" width="50" />
        <icon height="100" src="res/icons/Icon-50@2x.png" width="100" />
        <icon height="167" src="res/icons/Icon-83.5@2x.png" width="167" />
    </platform>
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-plugin-splashscreen" spec="^4.0.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
